<meta property="og:title" content="{{ title is defined ? title : '' }}" />
<meta property="og:url" content="{{ url is defined ? url : '' }}" />
<meta property="og:type" content="{{ type is defined ? type : '' }}" />
<meta property="og:image" content="{{ image is defined ? url(image) : '' }}" />
<meta property="og:description" content="{{ description is defined ? description : '' }}" />